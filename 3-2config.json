{
    "initial_url": "https://www.moeshome.com/",
    "config_links": {
        "initial_page_xpath": {
            "xpath": "//ul[@class='navbar-nav flex flex-row align-items-center']/li[position()<last()]//a"
        },
        "category1": {
            "xpath": "//div[@id='product-listing']/div[1]/div[2]/div//a"
        },
        "category2": {
            "xpath": ""
        },
        "products": {
            "xpath": "//div[@itemtype='http://schema.org/ItemList']/div//a[@class='p-0']"
        },
        "next_page": {
            "xpath": "//div[contains(@class,'pagination')]//a[contains(.,'Next')]/@class[not(contains(.,'disabled'))]/parent::a"
        }
    },
    "config_others": {
        "view_all": {
            "xpath": "//span[normalize-space()='View all']"
        },
        "variants_flag": {
            "xpath": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div"
        },
        "variants": {
            "xpath": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div"
        },
        "cookie": {
            "xpath": "//button[contains(@class,'white moes-btn undefined')]"
        }
    },
    "config_products": {
        "product_code": {
            "xpath": "//div[@id='spec-row']//td[normalize-space()='SKU']/following-sibling::td"
        },
        "product_name": {
            "xpath": "//div[@class='product-name']/h2"
        },
        "category1": {
            "xpath": ""
        },
        "category2": {
            "xpath": ""
        },
        "description": {
            "xpath": "//div[@class='overview-col-wrapper']"
        },
        "image1": {
            "xpath": "//div[@class='slick-track']/div[1]//picture/source[1]"
        },
        "images": {
            "xpath": "//div[@class='slick-track']/div//picture/source[1]"
        }
    },
    "custom_attributes": {
