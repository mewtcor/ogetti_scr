        "variant2": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div[2]/div[1]/span[2]",
        "variant_label1": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div[1]/div[1]/span[1]",
        "variant_label2": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div[2]/div[1]/span[1]",
        "swatch_image1": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div[1]/div[2]/div/@class[contains(.,'active')]/parent::div/img",
        "swatch_image2": "//div[@id='product-arrangement-and-colors-wrapper']/div/div/div[2]/div[2]/div/@class[contains(.,'active')]/parent::div/img",
        "price": "//span[@itemprop='price']",
